{
  "version": 2,
  "builds": [
    {
      "src": "dist/server/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/server/entry.mjs"
    }
  ]
}