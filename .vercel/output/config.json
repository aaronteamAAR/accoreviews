{
  "version": 3,
  "routes": [
    {
      "src": "^/(.*)$",
      "dest": "/.vercel_output/functions/_render.func"
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/_astro/(.*)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "^/_server-islands/([^/]+?)/?$",
      "dest": "_render"
    },
    {
      "src": "^/_image/?$",
      "dest": "_render"
    },
    {
      "src": "^/about/?$",
      "dest": "_render"
    },
    {
      "src": "^/blog/([^/]+?)/?$",
      "dest": "_render"
    },
    {
      "src": "^/blog/?$",
      "dest": "_render"
    },
    {
      "src": "^/$",
      "dest": "_render"
    }
  ],
  "framework": {
    "version": "5.1.10"
  },
  "crons": []
}
